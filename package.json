{
  "name": "schema-org",
  "version": "0.2.1",
  "description": "A node.js library that retrieves, parses and provides all schemas from schema.org",
  "author": "Charlie Robbins <charlie.robbins@gmail.com>",
  "repository": {
    "type": "git",
    "url": "http://github.com/indexzero/node-schema-org.git" 
  },
  "keywords": ["schemas", "jsdom", "parsers"],
  "dependencies": {
    "async": "0.1.x",
    "cheerio" :"*",
    "colors": "0.6.0-1",
    "cliff": "0.x.x",
    "jsdom": "0.3.x",
    "neuron": "0.4.x",
    "optimist": "0.3.x",
    "pkginfo": "0.2.x",
    "request": "2.x.x",
    "winston": "~0.6.2",
    "underscore" : "*",
    "underscore.string" : "*",
    "validator": "0.4.x"
  },
  "bin": {
    "read-schema-org": "./bin/read-schema-org",
    "parse-schema": "./bin/parse-schema",
    "list-schema": "./bin/list-schemas"
  },
  "main": "./lib",
  "engines": { "node": ">= 0.6.0" }
}
